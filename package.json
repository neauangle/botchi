{
  "name": "botchi",
  "version": "1.0.0",
  "description": "Bot suite for Binance and Ethereum-address-compatible chains.",
  "main": "app/main.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "electron --trace-warnings .",
    "pack": "electron-builder --dir",
